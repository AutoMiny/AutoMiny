<?xml version="1.0"?>
<package format="3">
    <name>odometry</name>
    <version>0.0.1</version>
    <description>Emergency stop using LiDAR</description>

    <maintainer email="s.sundermann@fu-berlin.de">Autonomos-Labs, FU Berlin</maintainer>

    <license>proprietary</license>

    <url type="website">https://www.autonomos-labs.de/</url>

    <author email="s.sundermann@fu-berlin.de">Autonomos-Labs, FU Berlin</author>

    <buildtool_depend>ament_cmake</buildtool_depend>

    <depend>rclcpp</depend>
    <depend>rclcpp_components</depend>
    <depend>nav_msgs</depend>
    <depend>autominy_msgs</depend>
    <depend>tf2</depend>
