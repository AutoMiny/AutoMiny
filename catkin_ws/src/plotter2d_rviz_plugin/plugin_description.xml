<library path="lib/libplotter2d_rviz_plugin">
    <class name="jsk_rviz_plugin/Plotter2D"
           type="jsk_rviz_plugins::Plotter2DDisplay"
           base_class_type="rviz::Display">
        <description>
            2D plotter on rviz
        </description>
        <message_type>autominy_msgs/Plot</message_type>
    </class>
    <class name="jsk_rviz_plugin/CameraInfoDisplay"
           type="jsk_rviz_plugins::CameraInfoDisplay"
           base_class_type="rviz::Display">
        <description>
            Camera info display
        </description>
        <message_type>sensor_msgs/CameraInfo</message_type>
    </class>
    <class name="jsk_rviz_plugin/TFTrajectoryDisplay"
           type="jsk_rviz_plugins::TFTrajectoryDisplay"
           base_class_type="rviz::Display">
        <description>
            Camera info display
        </description>
    </class>
    <class name="jsk_rviz_plugin/OverlayImageDisplay"
           type="jsk_rviz_plugins::OverlayImageDisplay"
           base_class_type="rviz::Display">
        <description>
            Overlay image display
        </description>
        <message_type>sensor_msgs/Image</message_type>
    </class>
    <class name="jsk_rviz_plugin/OverlayCameraDisplay"
           type="jsk_rviz_plugins::OverlayCameraDisplay"
           base_class_type="rviz::Display">
        <description>
            Overlay camera display
        </description>
        <message_type>sensor_msgs/Image</message_type>
    </class>
    <class name="jsk_rviz_plugin/TwistStampedDisplay"
           type="jsk_rviz_plugins::TwistStampedDisplay"
           base_class_type="rviz::Display">
        <description>
            Twist stamped display
        </description>
        <message_type>geometry_msgs/TwistStamped</message_type>
    </class>
</library>