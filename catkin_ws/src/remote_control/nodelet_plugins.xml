<!--
    relative workspace path to .so file (without file suffix),
    e.g. this path refers to catkin_ws/devel/lib/libremote_control_nodelet.so
-->
<library path="lib/libremote_control_nodelet">
    <!--
        name: value of variable nodeletName in RemoteControlNode.cpp
        type/base_class_type: first/second param of macro PLUGINLIB_EXPORT_CLASS in RemoteControlNodelet.cpp
    -->
    <class name="remote_control/Nodelet"
           type="remote_control::RemoteControlNodelet"
           base_class_type="nodelet::Nodelet"
    >
        <description>
            Short description what this nodelet is doing
        </description>
    </class>
</library>
